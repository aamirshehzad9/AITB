# AITB Environment Configuration Template
# Copy this file to AITB.env and fill in your actual API keys

# MCP Server Configuration
MCP_BASE_URL=http://localhost:8600

# Binance API Configuration
BINANCE_API_KEY=YOUR_BINANCE_API_KEY_HERE
BINANCE_SECRET_KEY=YOUR_BINANCE_SECRET_KEY_HERE
BINANCE_BASE_URL=https://api.binance.com
BINANCE_STREAM_URL=wss://stream.binance.com:9443/ws

# Inference API Configuration
INFERENCE_API_BASE=http://localhost:8001

# Google Studio API Configuration
GOOGLE_STUDIO_API_KEY=YOUR_GOOGLE_STUDIO_API_KEY_HERE

# CoinAPI Configuration
COINAPI_KEY=YOUR_COINAPI_KEY_HERE

# Hugging Face API Configuration
HUGGINGFACE_API_KEY_01=hf_YOUR_HUGGINGFACE_API_KEY_01_HERE
HUGGINGFACE_API_KEY_02=hf_YOUR_HUGGINGFACE_API_KEY_02_HERE
HUGGINGFACE_API_KEY_03=hf_YOUR_HUGGINGFACE_API_KEY_03_HERE

# Telegram Bot Configuration
TELEGRAM_BOT_NAME=YOUR_TELEGRAM_BOT_NAME
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE
TELEGRAM_USER_ID=YOUR_TELEGRAM_ID_HERE

# JWT Configuration
JWT_SECRET_KEY=YOUR_JWT_SECRET_KEY_HERE
JWT_ISSUER=AITB
JWT_AUDIENCE=AITB-Users
JWT_EXPIRY_MINUTES=60

# Database Configuration
DATABASE_CONNECTION_STRING=YOUR_DATABASE_CONNECTION_STRING_HERE

# Redis Configuration (for caching)
REDIS_CONNECTION_STRING=localhost:6379

# Environment Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://localhost:5000;https://localhost:5001