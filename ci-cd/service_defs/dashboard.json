{
  "serviceName": "dashboard",
  "displayName": "AITB Analytics Dashboard",
  "description": "Streamlit-based analytics and monitoring dashboard",
  "app": "python",
  "appParameters": "-m streamlit run app.py --server.port=8501 --server.address=localhost",
  "appDirectory": "D:\\apps\\aitb\\dashboard\\v1.0.0",
  "workingDirectory": "D:\\apps\\aitb\\dashboard\\v1.0.0",
  "environment": {
    "PYTHONPATH": "D:\\apps\\aitb\\dashboard\\v1.0.0",
    "PYTHONUNBUFFERED": "1",
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_SERVER_ADDRESS": "localhost"
  },
  "logging": {
    "stdOut": "D:\\logs\\aitb\\dashboard\\dashboard-stdout.log",
    "stdErr": "D:\\logs\\aitb\\dashboard\\dashboard-stderr.log",
    "rotationHint": "daily",
    "maxFileSizeMB": 100,
    "retentionDays": 30
  },
  "recovery": {
    "restartOnFailure": true,
    "maxRestartAttempts": 3,
    "restartDelaySeconds": 45,
    "backoffMultiplier": 2.0,
    "maxBackoffSeconds": 300,
    "resetCounterAfterSeconds": 3600
  },
  "healthCheck": {
    "endpoint": "http://localhost:8501/health",
    "intervalSeconds": 60,
    "timeoutSeconds": 15
  },
  "dependencies": {
    "configPath": "D:\\configs\\aitb\\dashboard\\config.json",
    "requiredServices": ["influxdb", "webapp"],
    "pythonRequirements": "D:\\apps\\aitb\\dashboard\\v1.0.0\\requirements.txt"
  }
}